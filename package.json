{
  "name": "@panosvoudouris/addon-versions",
  "version": "1.0.1",
  "description": "Navigate different versions of storybook style guides.",
  "main": "dist/register.js",
  "scripts": {
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9001 -s ./.storybook",
    "build-storybook": "build-storybook",
    "serve-static": "ws -d storybook-static",
    "lint": "eslint src example --ext .ts,.tsx",
    "transpile": "babel ./src --out-dir dist-babel --copy-files --ignore __mocks__,__snapshots__,test.js",
    "tsc": "tsc",
    "test": "jest ./src --coverage",
    "build": "npm run lint && npm run test && tsc",
    "prepublishOnly": "npm run tsc",
    "prepush": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panosvoudouris/storybook-addon-versions.git"
  },
  "bugs": {
    "url": "https://github.com/panosvoudouris/storybook-addon-versions/issues"
  },
  "homepage": "https://github.com/panosvoudouris/storybook-addon-versions#readme",
  "keywords": [
    "atomic-design",
    "react",
    "storybook",
    "storybook-addon",
    "storybook-addon-versions",
    "versions"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/components": "^5.3.19",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.4",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-loader": "^8.1.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.0.1",
    "fork-ts-checker-webpack-plugin": "^5.0.7",
    "jest": "^26.1.0",
    "local-web-server": "^4.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "source-map-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.0",
    "typescript": "^3.9.6",
    "webpack": "^4.29.6"
  },
  "peerDependencies": {
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.2.1"
  },
  "jest": {
    "coverageDirectory": "reports",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/register.tsx"
    ],
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/styleMock.ts"
    }
  }
}
